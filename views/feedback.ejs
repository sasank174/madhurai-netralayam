<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://kit.fontawesome.com/0e070a32e4.js"></script>
    <link rel="stylesheet" href="feedback.css">
    <title>feedback</title>
</head>

<body id="particles-js">
    <script src="particles.js"></script>
    <script src="app.js"></script>
    
    <header>
        <a href="/" class="logo">MN</a>

        <ul class="navigation">
            <li><a href="/">HOME</a></li>
            <li><a href="/about">ABOUT</a></li>
            <li><a href="/feedback">FEEDBACK</a></li>
            <li class="dropdown">
                <a href="/profile">PROFILE</a>
                <div class="dropdown-content">
                    <a href="#"><%= ename %></a>
                    <a href="/profile">profile</a>
                    <a href="/history">history</a>
                    <a href="/logout">sign out</a>
                </div>
            </li>
        </ul>
    </header>

    <section>
        <form action="/feedback" method="post">
            <div class="inputbox">
                <label>Email:</label>
                <input type="email" name="email" placeholder="email" required>
            </div>
            <div class="inputbox">
                <label>Comment:</label>
                <textarea name="comment" cols="30" rows="7" required
                    placeholder="provide your feedback to improve us"></textarea>
            </div>
            <div class="inputbox">
                <button type="submit">Submit</button>
            </div>
        </form>
    </section>

    <div class="media">
        <ul>
            <li><a href="https://en-gb.facebook.com/login/" target="_blank"><i class="fab fa-facebook-f"></i></a></li>
            <li><a href="https://www.instagram.com/accounts/login/" target="_blank"><i class="fab fa-twitter"></i></a></li>
            <li><a href="https://twitter.com/login?lang=en" target="_blank"><i class="fab fa-instagram"></i></a></li>
        </ul>
    </div>
</body>

</html>